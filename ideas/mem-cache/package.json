{
  "name": "mem-cache",
  "description": "Typed memory cache for stateless functions",
  "version": "0.0.0",
  "private": true,
  "author": "christian@hackyourshack.org",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "concurrently \"pnpm:build:*\"",
    "build:modules": "vite build",
    "build:types": "tsc",
    "clean": "rimraf -rf ./dist/",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.7.21",
    "@vitest/ui": "^0.23.2",
    "concurrently": "^7.4.0",
    "eslint-config-base": "workspace:*",
    "rimraf": "^3.0.2",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.3",
    "vite": "^3.1.1",
    "vite-config": "workspace:*",
    "vitest": "^0.23.2"
  }
}
