{
  "name": "astro-m2dx",
  "version": "0.6.2",
  "description": "Remark plugin to enhance MDX in the scope of Astro site generation",
  "author": "christian@hackyourshack.org",
  "homepage": "https://astro-m2dx.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/christian-hackyourshack/npm.git"
  },
  "license": "MIT",
  "keywords": [
    "remark-plugin",
    "astro",
    "markdown",
    "mdx"
  ],
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rimraf -rf ./dist/",
    "test": "mintest",
    "test:dev": "tsx watch src/relativeImages/index.spec.ts",
    "test:watch": "tsx watch node_modules/mintest-green/dist/bin.js"
  },
  "dependencies": {
    "acorn": "^8.8.0",
    "estree-util-visit": "^1.2.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "m2dx-utils": "workspace:*",
    "mdast-util-mdx": "^2.0.0",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "remark-mdx": "^2.1.5"
  },
  "devDependencies": {
    "@internal/utils": "workspace:*",
    "@types/estree": "^1.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.11.3",
    "@types/unist": "2.0.6",
    "concurrently": "^7.5.0",
    "esbuild": "^0.15.12",
    "esbuild-node-externals": "^1.5.0",
    "eslint-config-base": "workspace:*",
    "hast-util-to-html": "^8.0.3",
    "mdast-util-to-hast": "^12.2.4",
    "mintest-green": "workspace:*",
    "rehype-format": "^4.0.1",
    "rimraf": "^3.0.2",
    "tsconfig": "workspace:*",
    "tsup": "^6.3.0",
    "tsx": "^3.10.4",
    "typescript": "^4.8.4",
    "unified": "^10.1.2",
    "vfile": "^5.3.5"
  }
}
