{
  "name": "remark-importless-jsx",
  "version": "0.1.2",
  "description": "Remark plugin to automatically import defined JSX components",
  "author": "christian@hackyourshack.org",
  "homepage": "https://github.com/christian-hackyourshack/npm/tree/main/packages/remark-importless-jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/christian-hackyourshack/npm.git"
  },
  "license": "MIT",
  "keywords": [
    "remark-plugin",
    "JSX",
    "import"
  ],
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rimraf -rf ./dist/",
    "test": "mintest",
    "test:dev": "tsx watch src/index.spec.ts",
    "test:watch": "tsx watch node_modules/mintest-green/dist/bin.js"
  },
  "dependencies": {
    "pre-visit": "workspace:*"
  },
  "devDependencies": {
    "@internal/estree-util": "workspace:*",
    "@internal/mdast-util": "workspace:*",
    "@internal/mdast-util-mdx": "workspace:*",
    "@internal/utils": "workspace:*",
    "@types/node": "20.3.1",
    "eslint-config-base": "workspace:*",
    "mintest-green": "workspace:*",
    "preact": "10.15.1",
    "rehype-stringify": "^9.0.3",
    "remark-mdx": "^2.3.0",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "rimraf": "^3.0.2",
    "tsconfig": "workspace:*",
    "tsup": "^6.4.0",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4",
    "unified": "^10.1.2"
  }
}
